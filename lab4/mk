VFD=mtximage

as86 -o ts.o ts.s
bcc  -c -ansi t.c
#bcc  -c -ansi bio.c
#bcc  -c -ansi queue.c
#ld86 -d -o mtx ts.o t.o bio.o queue.o mtxlib /usr/lib/bcc/libc.a
ld86 -d -o mtx ts.o t.o mtxlib /usr/lib/bcc/libc.a

mount -o loop $VFD /mnt
cp mtx /mnt/boot
umount /mnt
rm *.o mtx

(cd USER; mku u1)

echo done
